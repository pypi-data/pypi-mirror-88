{
  "title": "{{title}}",
  "type": "histogram",
  "params": {
    "type": "histogram",
    "grid": {
      "categoryLines": false,
      "style": {
        "color": "#eee"
      }
    },
    "categoryAxes": [
      {
        "id": "CategoryAxis-1",
        "type": "category",
        "position": "bottom",
        "show": true,
        "style": {},
        "scale": {
          "type": "linear"
        },
        "labels": {
          "show": true,
          "truncate": 100,
          "rotate": 75
        },
        "title": {}
      }
    ],
    "valueAxes": [
      {
        "id": "ValueAxis-1",
        "name": "LeftAxis-1",
        "type": "value",
        "position": "left",
        "show": true,
        "style": {},
        "scale": {
          "type": "linear",
          "mode": "normal"
        },
        "labels": {
          "show": true,
          "rotate": 0,
          "filter": false,
          "truncate": 100
        },
        "title": {
          "text": "Время, мсек"
        }
      }
    ],
    "seriesParams": [
      {
        "show": true,
        "mode": "stacked",
        "type": "line",
        "drawLinesBetweenPoints": true,
        "showCircles": true,
        "interpolate": "cardinal",
        "data": {
          "id": "4",
          "label": "Время SQL"
        },
        "valueAxis": "ValueAxis-1"
      },
      {
        "show": true,
        "mode": "stacked",
        "type": "line",
        "drawLinesBetweenPoints": true,
        "showCircles": true,
        "interpolate": "cardinal",
        "data": {
          "id": "2",
          "label": "Время серверное"
        },
        "valueAxis": "ValueAxis-1"
      },
      {
        "show": "true",
        "type": "line",
        "mode": "stacked",
        "data": {
          "label": "Время клиентское",
          "id": "1"
        },
        "valueAxis": "ValueAxis-1",
        "drawLinesBetweenPoints": true,
        "showCircles": true,
        "interpolate": "cardinal"
      }
    ],
    "addTooltip": true,
    "addLegend": true,
    "legendPosition": "right",
    "times": [],
    "addTimeMarker": false
  },
  "aggs": [
    {
      "id": "3",
      "enabled": true,
      "type": "date_histogram",
      "schema": "segment",
      "params": {
        "field": "@timestamp",
        "timeRange": {
          "from": "now/d",
          "to": "now/d",
          "mode": "quick"
        },
        "useNormalizedEsInterval": true,
        "interval": "h",
        "time_zone": "Europe/Moscow",
        "drop_partials": false,
        "customInterval": "2h",
        "min_doc_count": 1,
        "extended_bounds": {}
      }
    },
    {
      "id": "4",
      "enabled": true,
      "type": "avg",
      "schema": "metric",
      "params": {
        "field": "sql_total",
        "customLabel": "Время SQL"
      }
    },
    {
      "id": "2",
      "enabled": true,
      "type": "avg",
      "schema": "metric",
      "params": {
        "field": "total",
        "customLabel": "Время серверное"
      }
    },
    {
      "id": "1",
      "enabled": true,
      "type": "avg",
      "schema": "metric",
      "params": {
        "field": "c_total",
        "customLabel": "Время клиентское"
      }
    }
  ]
}