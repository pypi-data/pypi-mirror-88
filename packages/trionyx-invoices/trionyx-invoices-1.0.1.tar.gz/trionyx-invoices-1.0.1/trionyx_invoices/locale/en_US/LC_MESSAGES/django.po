#, python-format
msgid ""
"\n"
"    We are contacting you in regard to a new invoice #%(reference)s that has "
"been created on your account\n"
msgstr ""

#: trionyx_invoices/templates/invoices/emails/invoice.html:13
#, python-format
msgid ""
"\n"
"    You may find the invoice attached. Please pay the balance of "
"%(grand_total)s by %(due_date)s\n"
