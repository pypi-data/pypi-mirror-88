    image: url(:/icon/primary/slider.svg);
    width: 20px;
    height: 20px;
    margin: -20px -10px;
}
.QSlider::handle:vertical {

    image: url(:/icon/primary/slider.svg);
    width: 20px;
    height: 20px;
    margin: -10px -20px;
}





