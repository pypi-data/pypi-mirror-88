    subcontrol-origin: border;
    subcontrol-position: bottom right; /* position at bottom right corner */

    width: 20px;
    image: url(:/icon/primary/downarrow.svg);
    border-width: 0px;
    border-top-width: 0;
    margin-right: 5px;
}


