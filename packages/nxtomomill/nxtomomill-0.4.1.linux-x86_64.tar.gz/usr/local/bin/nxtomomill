#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'nxtomomill==0.4.1','gui_scripts','nxtomomill'
__requires__ = 'nxtomomill==0.4.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('nxtomomill==0.4.1', 'gui_scripts', 'nxtomomill')()
    )
